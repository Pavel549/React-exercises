// Create a "getDigitsSum" function that will find the sum of the digits of the passed number. 
// Use it to count the sum of the digits of the number 12345.




import React from 'react';

function App() {
	
	function getDigitSum(num) {
		let arr = String(num).split('');
		let sum = 0;
		  for (let elem of arr) {
			sum += Number(elem);
		}
		return sum;
	}

return <div>

	{getDigitSum(12345)}

</div>

}

export default App;
